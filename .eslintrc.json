{
  "env": {
    "browser": false,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:node/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "node/no-unsupported-features/es-syntax": "off"
  },
  "plugins": ["node"]
}
